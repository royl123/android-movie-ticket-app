<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.Luengas_Movie_Tickets">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Roy's Movie Tickets"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Final_Project">

        <activity
            android:name="com.example.Luengas_Movie_Tickets.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.example.Luengas_Movie_Tickets.info"
            android:parentActivityName="com.example.Luengas_Movie_Tickets.MainActivity"/>

        <activity android:name="com.example.Luengas_Movie_Tickets.Checkout"
            android:parentActivityName="com.example.Luengas_Movie_Tickets.info" />
    </application>

</manifest>